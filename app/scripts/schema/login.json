{
  "type": "object",
  "properties": {
    "emailAddress": {
      "type": "string",
      "pattern": "^([A-Za-z0-9\\+_\\-]+)(\\.[A-Za-z0-9\\+_\\-]+)*@([A-Za-z0-9\\-]+\\.)+[A-Za-z]{2,6}$"
    },
    "password": {
      "type": "string"
    },
    "rememberMe": {
      "type": "boolean",
      "default": true
    }
  },
  "required": [
    "emailAddress",
    "password"
  ]
}
